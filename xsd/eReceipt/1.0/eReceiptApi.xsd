      <xs:extension base="base:BasicEReceiptResponseType">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Az eReceipt rendszerben alkalmazott általános válasz típus adatai.</xs:documentation>
          <xs:documentation xml:lang="en">The general response type of the eReceipt system.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="authenticationPrivateKey" type="communication:PrivateKeyType">
            <xs:annotation>
              <xs:documentation xml:lang="hu">Authentikációs tanúsítvány privát kulcsa. Ezzel a tanúsítvánnyal tud bejelentkezni az e-pénztárgép. Az eszközregisztrációs szolgáltatáson kívül minden más szolgáltatás esetén használni kell ezt az authentikációs tanúsítványt. Az authentikációs tanúsítvány felhasználásának részletei a Fejlesztői dokumentáció „Authentikáció” fejezetében találhatók.</xs:documentation>
              <xs:documentation xml:lang="en">The private key of the authentication key. This key has to be used when other services called except the device registration. For further information see "Authentication" chapter in the Development Documentation.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="authenticationPublicKey" type="communication:PublicKeyType">
            <xs:annotation>
              <xs:documentation xml:lang="hu">Authentikációs tanúsítvány publikus kulcsa.</xs:documentation>
              <xs:documentation xml:lang="en">The public key of the authentication key.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="vatList" type="communication:VatListType">
            <xs:annotation>
              <xs:documentation xml:lang="hu">Az e-pénztárgépen alkalmazandó ÁFA kulcsok és gyűjtők listája.</xs:documentation>
              <xs:documentation xml:lang="en">The VAT key and collector list used by e-cash register.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="teaorList" type="communication:TeaorCodeListType">
            <xs:annotation>
              <xs:documentation xml:lang="hu">Az üzlet TEÁOR szerinti tevékenységeinek listája.</xs:documentation>
              <xs:documentation xml:lang="en">The shop function by TEAOR list.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="operatorSite" type="communication:OperatorSiteType">
            <xs:annotation>
              <xs:documentation xml:lang="hu">Az e-pénztárgép üzemeltetői és telephely adatai.</xs:documentation>
              <xs:documentation xml:lang="en">The operator and site data of the e-cash register.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
