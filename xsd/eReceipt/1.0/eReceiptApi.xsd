<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema targetNamespace="http://schemas.nav.gov.hu/ERECEIPT/1.0/api" elementFormDefault="qualified"
           attributeFormDefault="unqualified" xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns="http://schemas.nav.gov.hu/ERECEIPT/1.0/api" xmlns:common="http://schemas.nav.gov.hu/NTCA/1.0/common"
           xmlns:communication="http://schemas.nav.gov.hu/ERECEIPT/1.0/communicationData"
           xmlns:reportMessage="http://schemas.nav.gov.hu/ERECEIPT/1.0/reportMessage"
           xmlns:base="http://schemas.nav.gov.hu/ERECEIPT/1.0/base" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xs:import namespace="http://schemas.nav.gov.hu/NTCA/1.0/common" schemaLocation="common.xsd"/>
  <xs:import namespace="http://schemas.nav.gov.hu/ERECEIPT/1.0/base" schemaLocation="eReceiptBase.xsd"/>
  <xs:import namespace="http://schemas.nav.gov.hu/ERECEIPT/1.0/communicationData"
             schemaLocation="communicationData.xsd"/>
  <xs:annotation>
    <xs:documentation xml:lang="hu">Az e-pénztárgépek által hívható szolgáltatások kérés-válasz objektumait leíró XSD.</xs:documentation>
    <xs:documentation xml:lang="en">The request and response objects of the services called be e-cash registers.</xs:documentation>
  </xs:annotation>  
  <xs:complexType name="BlockUnblockRequestType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Az e-pénztárgép blokkolását vagy blokkolásának feloldását megvalósító szolgáltatás kérés típusa.</xs:documentation>
      <xs:documentation xml:lang="en">The e-cash register blocking and unblocking service request type.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="base:BasicEReceiptRequestType">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Az eReceipt rendszerben alkalmazott általános kérés típus adatai.</xs:documentation>
          <xs:documentation xml:lang="en">The general request type of the eReceipt system.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="requestedOperation" minOccurs="0" type="communication:BlockUnblockType">
            <xs:annotation>
              <xs:documentation xml:lang="hu">Amennyiben az e-pénztárgép blokkolása vagy blokkolásának feloldása az e-pénztárgép kérésére történik, akkor a kért művelet típusa.</xs:documentation>
              <xs:documentation xml:lang="en">In case the e-cash register request the blocking or unblocking operation then the operation type.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="BlockUnblockResponseType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Az e-pénztárgép blokkolását vagy blokkolásának feloldását megvalósító szolgáltatás válasz típusa.</xs:documentation>
      <xs:documentation xml:lang="en">The e-cash register blocking and unblocking service response type.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="base:BasicEReceiptResponseType">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Az eReceipt rendszerben alkalmazott általános válasz típus adatai.</xs:documentation>
          <xs:documentation xml:lang="en">The general response type of the eReceipt system.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="blockUnblock" type="communication:BlockUnblockType">
            <xs:annotation>
              <xs:documentation xml:lang="hu">Utasítás az e-pénztárgép blokkolására vagy blokkolásának feloldására. BLOCK esetén az e-pénztárgépet blokkolt állapotba kell állítani, UNBLOCK esetén az e-pénztárgép blokkolását fel kell oldani.</xs:documentation>
              <xs:documentation xml:lang="en">Command to block to unblock the e-cash register. In case of BLOCK the e-cash register has to be blocked, in case of UNBLOCK the e-cash register has to be unblocked.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="CommMgrRequestType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Az e-pénztárgép által meghívandó szolgáltatások listáját visszaadó szolgáltatás kérés típusa.</xs:documentation>
      <xs:documentation xml:lang="en">The request type of the service which lists the services that the e-cash register has to call.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="base:BasicEReceiptRequestType">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Az eReceipt rendszerben alkalmazott általános kérés típus adatai.</xs:documentation>
          <xs:documentation xml:lang="en">The general request type of the eReceipt system.</xs:documentation>
        </xs:annotation>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="CommMgrResponseType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Az e-pénztárgép által meghívandó szolgáltatások listáját visszaadó szolgáltatás válasz típusa.</xs:documentation>
      <xs:documentation xml:lang="en">The response type of the service which lists the services that the e-cash register has to call.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="base:BasicEReceiptResponseType">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Az eReceipt rendszerben alkalmazott általános válasz típus adatai.</xs:documentation>
          <xs:documentation xml:lang="en">The general response type of the eReceipt system.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="URLs" type="communication:UrlsListType">
            <xs:annotation>
              <xs:documentation xml:lang="hu">Az e-pénztárgép által meghívandó szolgáltatások listája.</xs:documentation>
              <xs:documentation xml:lang="en">The list of the services that the e-cash register has to call.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="DocumentRequestType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">A nyugta, egyszerűsített számla, és egyes speciális értékesítések bizonylatainak fogadását megvalósító szolgáltatás kérés típusa.</xs:documentation>
      <xs:documentation xml:lang="en">The request type of the service which receives receipts, simplified invoices and special sale documents.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="base:BasicEReceiptRequestType">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Az eReceipt rendszerben alkalmazott általános kérés típus adatai.</xs:documentation>
          <xs:documentation xml:lang="en">The general request type of the eReceipt system.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="taxNumber" type="common:TaxpayerIdType">
            <xs:annotation>
              <xs:documentation xml:lang="hu">A bizonylatot kiállító adózó adószámának első 8 karaktere.</xs:documentation>
              <xs:documentation xml:lang="en">The first 8 characters of the tax number of the taxpayer who creates the document.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="documentClass" type="base:DocumentClassType">
            <xs:annotation>
              <xs:documentation xml:lang="hu">A beküldött bizonylat típusa (nyugta, egyszerűsített számla, stb.)</xs:documentation>
              <xs:documentation xml:lang="en">The type of the document (recipt, simplified invoce, etc.)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ntcaDocumentEnvelope" type="base:SignedEnvelopeType">
            <xs:annotation>
              <xs:documentation xml:lang="hu">A bizonylat NAV-nak átadandó adatai a Fejlesztői dokumentációban meghatározott formátumban.</xs:documentation>
              <xs:documentation xml:lang="en">The document data for NTCA in the format described in Development Documentation.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="customerDocumentEnvelope" type="base:SignedEnvelopeType">
            <xs:annotation>
              <xs:documentation xml:lang="hu">A bizonylat vevőnek átadandó adatai a Fejlesztői dokumentációban meghatározott formátumban.</xs:documentation>
              <xs:documentation xml:lang="en">The document data for the customer in the format described in Development Documentation.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="searchKeyTimestamp" type="common:GenericTimestampType" minOccurs="1">
            <xs:annotation>
              <xs:documentation xml:lang="hu">A nyugtatárból ezzel az időbélyeggel kérdezhető le a bizonylat. A bizonylat kiállításának időpontját tartalmazza.</xs:documentation>
              <xs:documentation xml:lang="en">The document can be queried by this data from the Document Store. It contains the document creation timestamp.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="searchKey" type="xs:base64Binary">
            <xs:annotation>
              <xs:documentation xml:lang="hu">A nyugtatárból ezzel a kulccsal kérdezhető le a bizonylat.</xs:documentation>
              <xs:documentation xml:lang="en">The document can be queried by this data from the Document Store.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="isQRCodeExpired" type="xs:boolean">
            <xs:annotation>
              <xs:documentation xml:lang="hu">A QR generálása a fejlesztői dokumentációban meghatározott időintervallumnál régebben készült .</xs:documentation>
              <xs:documentation xml:lang="en">QR code generate timestamp expired.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="isOfflineCreated" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="hu">A hírközlő hálózat elérhetetlenségének megszűnése esetén az érintett bizonylatokat és adatszolgáltatásokat haladéktalanul el kell küldeni, a késleltetett küldés tényét ennek a mezőnek a kitöltésével kell jelezni.</xs:documentation>
              <xs:documentation xml:lang="en">Whether the document was created offline. When the communication network is online again, the document has to be sent immidiately, with this flag.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="cashRegisterSignPublicKey" type="communication:PublicKeyType">
            <xs:annotation>
              <xs:documentation xml:lang="hu">Az e-pénztárgép aláíró kulcspárjának a publikus kulcsa PEM formátumban. Ezzel a kulccsal kerülnek aláírásra az e-pénztárgép által küldött üzenetek. Az aláíró kulccsal kapcsolatos követelmények a Fejlesztői dokumentáció „Aláírás képzése” fejezetében találhatók.</xs:documentation>
              <xs:documentation xml:lang="en">The public key of the e-cash register sign key in PEM format. This key has to be used to sign the messages of the e-cash register. The requirements of the sign key can be found in the "Signing" chapter of the Development Documentation.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="recordCounter" type="xs:nonNegativeInteger">
            <xs:annotation>
              <xs:documentation xml:lang="hu">Az aktuálisan beküldött bizonylat recordCounter értéke. Leírását lásd a Fejlesztői dokumentáció „NAV ellenőrző kód képzése” fejezetében.</xs:documentation>
              <xs:documentation xml:lang="en">The recordCounter value of the actual document. See further information in the "Generate NAV verification code" chapter of the Development Documentation.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="lastRecordCounter" type="xs:nonNegativeInteger">
            <xs:annotation>
              <xs:documentation xml:lang="hu">Az e-pénztárgépen utoljára kiállított bizonylat recordCounter értéke. Leírását lásd a Fejlesztői dokumentáció „NAV ellenőrző kód képzése” fejezetében.</xs:documentation>
              <xs:documentation xml:lang="en">The recordCounter value of the last document in the e-cash register. See further information in the "Generate NAV verification code" chapter of the Development Documentation.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ntcaVerificationCode" type="common:SHA256Type">
            <xs:annotation>
              <xs:documentation xml:lang="hu">NAV ellenőrző kód. Leírását lásd a Fejlesztői dokumentáció „NAV ellenőrző kód képzése” fejezetében.</xs:documentation>
              <xs:documentation xml:lang="en">Ntca verification code. See further information in the "Generate NAV verification code" chapter of the Development Documentation.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="DocumentResponseType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">A nyugta, egyszerűsített számla, és egyes speciális értékesítések bizonylatainak fogadását megvalósító szolgáltatás válasz típusa.</xs:documentation>
      <xs:documentation xml:lang="en">The response type of the service which receives receipts, simplified invoices and special sale documents.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="base:BasicEReceiptResponseType">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Az eReceipt rendszerben alkalmazott általános válasz típus adatai.</xs:documentation>
          <xs:documentation xml:lang="en">The general response type of the eReceipt system.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="taxNumber" type="common:TaxpayerIdType">
            <xs:annotation>
              <xs:documentation xml:lang="hu">A kérésben szereplő adószám.</xs:documentation>
              <xs:documentation xml:lang="en">The tax number in the request.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EndOfOperationRequestType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Az e-pénztárgép üzemeltetés befejezés szolgáltatás kérés típusa.</xs:documentation>
      <xs:documentation xml:lang="en">The request type of the e-cash register end of operation service.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="base:BasicEReceiptRequestType">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Az eReceipt rendszerben alkalmazott általános kérés típus adatai.</xs:documentation>
          <xs:documentation xml:lang="en">The general request type of the eReceipt system.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="lastRecordCounter" type="xs:nonNegativeInteger">
            <xs:annotation>
              <xs:documentation xml:lang="hu">Az e-pénztárgépen az utoljára kiállított bizonylat vagy riport recordCounter értéke.</xs:documentation>
              <xs:documentation xml:lang="en">The recordCounter value of the last receipt or report issued at the e-cash register.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="coTaxpayerLastRecordCounter" minOccurs="0" type="xs:nonNegativeInteger">
            <xs:annotation>
              <xs:documentation xml:lang="hu">Kétadózós e-pénztárgép esetén a társadózó által az e-pénztárgépen utoljára kiállított bizonylat vagy riport recordCounter értéke.</xs:documentation>
              <xs:documentation xml:lang="en">In case of two taxpayers e-cash register, the recordCounter value of the last receipt or report issued by the co-taxpayer.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="cashRegisterWorkState" type="communication:CashRegisterWorkStateType">
            <xs:annotation>
              <xs:appinfo>
                <legacyMapping/>
              </xs:appinfo>
              <xs:documentation xml:lang="hu">Az e-pénztárgép állapota</xs:documentation>
              <xs:documentation xml:lang="en">The state of the e-cash register</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EndOfOperationResponseType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Az e-pénztárgép üzemeltetés befejezés szolgáltatás válasz típusa.</xs:documentation>
      <xs:documentation xml:lang="en">The response type of the e-cash register end of operation service.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="base:BasicEReceiptResponseType">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Az eReceipt rendszerben alkalmazott általános válasz típus adatai.</xs:documentation>
          <xs:documentation xml:lang="en">The general response type of the eReceipt system.</xs:documentation>
        </xs:annotation>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="HelloRequestType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Az e-pénztárgép regisztrálását vagy átszemélyesítését követően meghívandó szolgáltatás kérés típusa.</xs:documentation>
      <xs:documentation xml:lang="en">The request type of the service which has to be called by the e-cash register after the device registration or the owner change.</xs:documentation>
