    <xs:complexContent>
      <xs:extension base="base:BasicEReceiptRequestType">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Az eReceipt rendszerben alkalmazott általános kérés típus adatai.</xs:documentation>
          <xs:documentation xml:lang="en">The general request type of the eReceipt system.</xs:documentation>
        </xs:annotation>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="HelloResponseType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Az e-pénztárgép regisztrálását vagy átszemélyesítését követően meghívandó szolgáltatás válasz típusa.</xs:documentation>
      <xs:documentation xml:lang="en">The response type of the service which has to be called by the e-cash register after the device registration or the owner change.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="base:BasicEReceiptResponseType">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Az eReceipt rendszerben alkalmazott általános válasz típus adatai.</xs:documentation>
          <xs:documentation xml:lang="en">The general response type of the eReceipt system.</xs:documentation>
        </xs:annotation>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="OperatorSiteUpdateRequestType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Az e-pénztárgépen az üzemeltetői vagy telephely adatok frissítését megvalósító szolgáltatás kérés típusa.</xs:documentation>
      <xs:documentation xml:lang="en">The operator or site data update service request type.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="base:BasicEReceiptRequestType">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Az eReceipt rendszerben alkalmazott általános kérés típus adatai.</xs:documentation>
          <xs:documentation xml:lang="en">The general request type of the eReceipt system.</xs:documentation>
        </xs:annotation>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="OperatorSiteUpdateResponseType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Az e-pénztárgépen az üzemeltetői vagy telephely adatok frissítését megvalósító szolgáltatás válasz típusa.</xs:documentation>
      <xs:documentation xml:lang="en">The operator or site data update service response type.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="base:BasicEReceiptResponseType">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Az eReceipt rendszerben alkalmazott általános válasz típus adatai.</xs:documentation>
          <xs:documentation xml:lang="en">The general response type of the eReceipt system.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="OperatorSite" type="communication:OperatorSiteType">
            <xs:annotation>
              <xs:documentation xml:lang="hu">Az e-pénztárgép üzemeltetői és telephely adatai.</xs:documentation>
              <xs:documentation xml:lang="en">The operator or site data of the e-cash register.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="OwnerChangeRequestType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Az e-pénztárgép átszemélyesítését kezdeményező szolgáltatás kérés típusa.</xs:documentation>
      <xs:documentation xml:lang="en">The request type of the e-cash register owner change service.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="base:BasicEReceiptRequestType">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Az eReceipt rendszerben alkalmazott általános kérés típus adatai.</xs:documentation>
          <xs:documentation xml:lang="en">The general request type of the eReceipt system.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="registrationNumber" type="communication:RegistrationNumberType">
            <xs:annotation>
              <xs:documentation xml:lang="hu">Üzembehelyezési kód. Annak az adózónak az adatait tartalmazza, amelyre az e-pénztárgép szeretne átszemélyesedni.</xs:documentation>
              <xs:documentation xml:lang="en">Registration number. It belongs to the taxpayer who will be the new owner of the e-cash register.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="lastRecordCounter" type="xs:nonNegativeInteger">
            <xs:annotation>
              <xs:documentation xml:lang="hu">Az e-pénztárgépen az utoljára kiállított bizonylat vagy riport recordCounter értéke.</xs:documentation>
              <xs:documentation xml:lang="en">The recordCounter value of the last receipt or report issued at the e-cash register.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="coTaxpayerLastRecordCounter" minOccurs="0" type="xs:nonNegativeInteger">
            <xs:annotation>
              <xs:documentation xml:lang="hu">Kétadózós e-pénztárgép esetén a társadózó által az e-pénztárgépen utoljára kiállított bizonylat vagy riport recordCounter értéke.</xs:documentation>
              <xs:documentation xml:lang="en">In case of two taxpayers e-cash register, the recordCounter value of the last receipt or report issued by the co-taxpayer.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="cashRegisterWorkState" type="communication:CashRegisterWorkStateType">
            <xs:annotation>
              <xs:appinfo>
                <legacyMapping/>
              </xs:appinfo>
              <xs:documentation xml:lang="hu">Az e-pénztárgép állapota</xs:documentation>
              <xs:documentation xml:lang="en">The state of the e-cash register.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="OwnerChangeResponseType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Az e-pénztárgép átszemélyesítését kezdeményező szolgáltatás válasz típusa.</xs:documentation>
      <xs:documentation xml:lang="en">The response type of the e-cash register owner change service.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="base:BasicEReceiptResponseType">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Az eReceipt rendszerben alkalmazott általános válasz típus adatai.</xs:documentation>
          <xs:documentation xml:lang="en">The general response type of the eReceipt system.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="OperatorSite" type="communication:OperatorSiteType">
            <xs:annotation>
              <xs:documentation xml:lang="hu">Az e-pénztárgép üzemeltetői és telephely adatai.</xs:documentation>
              <xs:documentation xml:lang="en">The operator and site date of the e-cash register.</xs:documentation>
            </xs:annotation>
